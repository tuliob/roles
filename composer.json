{
    "name": "tuliob/roles",
    "description": "Powerful package for handling roles and permissions in Laravel 5.",
    "abandoned": true,
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ultraware"
        },
        {
            "name": "tuliob"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^10.10",
        "illuminate/database": "^10.10"
    },
    "autoload": {
        "psr-4": {
            "TulioB\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "orchestra/database": "^3.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0",
        "satooshi/php-coveralls": "^2.0",
        "sebastian/environment": "^4.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Endone777\\Roles\\RolesServiceProvider"
            ]
        }
    }
}
